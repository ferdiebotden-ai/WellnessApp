{
  "name": "wellness-os-test-auth-function",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "functions-framework --target=testAuthHandler --source=dist/index.js --port=8080",
    "deploy": "gcloud functions deploy test-auth --gen2 --runtime=nodejs20 --region=$GCP_REGION --entry-point=testAuthHandler --trigger-http --allow-unauthenticated --set-env-vars=$(cat .env | xargs | tr ' ' ',')"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "functions-framework": "^3.4.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "typescript": "^5.4.5"
  }
}
