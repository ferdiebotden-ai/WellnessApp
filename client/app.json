{
  "expo": {
    "name": "Wellness OS",
    "slug": "wellness-os",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1218"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wellnessos.app",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Apex OS analyzes your HRV, sleep, and activity data to personalize evidence-based recovery protocols.",
        "NSHealthUpdateUsageDescription": "Apex OS records completed protocols and activity to Apple Health.",
        "UIBackgroundModes": [
          "processing"
        ]
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F1218"
      },
      "package": "com.wellnessos.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "scheme": "wellnessos",
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f5c39858-7d01-4a73-890a-311d6be683e1"
      }
    },
    "owner": "ferdie.botden"
  }
}
